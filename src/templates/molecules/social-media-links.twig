<div class="social-media-links">
    <span>
        <img src="/assets/scoutguide.png" class="custom-icon-scout" alt="social media link for scout guide">
    </span>
    {% for item in socialMediaLinks %}
        <a href="{{ item.link }}" target="_blank" class="social-icon si-colored si-{{ item.media }}">
            <img src="/assets/{{ item.media }}.png" class="custom-icon-{{ item.media }}" alt="social media link for {{ item.media }}">
        </a>
    {% endfor %}
</div>